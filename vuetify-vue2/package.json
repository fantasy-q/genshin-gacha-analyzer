{
  "name": "vuetify-vue2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fontmin": "node scripts/compressFont.js",
    "start": "vue-cli-service serve",
    "serve": "node scripts/compressFont.js && vue-cli-service serve",
    "build": "node scripts/compressFont.js && vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "dateformat": "^4.5.1",
    "fontmin": "^0.9.8",
    "lodash": "^4.17.21",
    "vue": "^2.6.11",
    "vuetify": "^2.4.0",
    "vuex": "^3.6.2",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "~1.32.0",
    "sass-loader": "^10.0.0",
    "vue-cli-plugin-vuetify": "~2.4.1",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "semi": [
        "warn",
        "never"
      ],
      "comma-dangle": [
        "warn",
        "always-multiline"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
